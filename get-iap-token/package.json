  "description": "GitHub action for getting a JWT that can be used to authenticate API calls behind a Goocle Cloud Identity Aware Proxy (IAP).",
  "main": "index.js",
  "scripts": {
    "build": "ncc build src/main.ts",
    "format": "prettier --write **/*.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fcoclavero/github-actions/tree/master/get-iap-token"
  },
  "keywords": [
    "actions",
    "google cloud",
    "gcloud",
    "sdk",
    "cloud sdk",
    "setup",
    "iap"
  ],
  "author": "fcoclavero",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "axios": "^0.19.2",
